{
    "name": "poweredlocal/vrata",
    "description": "API gateway written in PHP7 and Lumen",
    "keywords": [
        "microservices",
        "gateway",
        "lumen"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "appzcoder/lumen-routes-list": "^1.1",
        "barryvdh/laravel-cors": "^0.11.4",
        "dusterio/lumen-passport": "^0.2.15",
        "elcobvg/laravel-opcache": "^0.4.0",
        "illuminate/mail": "^6.8",
        "illuminate/redis": "^6.8",
        "jenssegers/mongodb": "^3.6",
        "laravel/helpers": "^1.1",
        "laravel/lumen-framework": "^6.0",
        "league/fractal": "^0.18.0",
        "predis/predis": "^1.1",
        "vlucas/phpdotenv": "3.6",
        "webpatser/laravel-uuid": "2.*",
        "league/flysystem": "^1.0",
        "luqta/laravel-passport-mongodb": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0",
        "codeclimate/php-test-reporter": "dev-master",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
